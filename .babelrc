{
  "sourceType": "unambiguous",
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-macros",
      {
        "twin": {
          "preset": "emotion"
        }
      }
    ],
    "@emotion/babel-plugin"
  ]
}
